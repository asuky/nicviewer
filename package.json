{
  "name": "nicviewer",
  "version": "0.0.1",
  "description": "NIC Viewer",
  "main": "dist/main.js",
  "scripts": {
    "script-build": "webpack --mode=production",
    "package": "electron-packager . nicviewer --icon=icon.ico --platform=darwin,win32 --arch=x64 --overwrite",
    "build": "npm run script-build && npm run package",
    "watch": "webpack --progress --watch --config webpack.config.js -p --color",
    "dev": "webpack --progress --config webpack.config.js -p --color",
    "ele": "electron .",
    "start": "npm run watch & npm run ele"
  },
  "author": "Asuki Yamashita",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "electron": "^3.0.8",
    "electron-is-dev": "^1.0.1",
    "electron-rebuild": "^1.8.2",
    "gkm": "^0.2.0",
    "iconv-lite": "^0.4.24",
    "node-gyp": "^3.8.0",
    "powershell": "^2.3.0",
    "react": "^16.6.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.1",
    "electron-packager": "^12.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "htmllint-loader": "^2.1.4",
    "jsxobj": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  }
}
